public class SetColletcion {
    //An Unordered colletcion of elements
    //It does not contain duplicate elements
    public void myMethod() {
        Set<String> stringSet = new set<String>();
        stringSet.add('Bruno');
        stringSet.add('Bruno');
        stringSet.add('Moreira');
        stringSet.add('Moreira');
        System.debug('My set collection: ' + stringSet);
    }
}